{
    "builds": [
      {
        "src": "vercel_app.py", 
        "use": "@vercel/python"
      },
      {
        "src": "static/**",
        "use": "@vercel/static"
      },
      {
        "src": "assets/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/assets/(.*)",
        "dest": "/assets/$1"
      },
      {
        "src": "/favicon.ico",
        "dest": "/assets/favicon.ico"
      },
      {
        "src": "/favicon.png", 
        "dest": "/assets/favicon.png"
      },
      {
        "src": "/(.*)",
        "dest": "/vercel_app.py"
      }
    ],
    "env": {
      "DEBUG": "False",
      "DJANGO_SETTINGS_MODULE": "socialdots.minimal_settings"
    }
}