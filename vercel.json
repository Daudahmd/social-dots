{
    "version": 2,
    "name": "social-dots-django-deployment",
    "builds": [
      {
        "src": "wsgi_handler.py",
        "use": "@vercel/python",
        "config": { 
          "maxLambdaSize": "50mb", 
          "runtime": "python3.11"
        }
      },
      {
        "src": "staticfiles/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/staticfiles/$1"
      },
      {
        "src": "/(.*)",
        "dest": "wsgi_handler.py"
      }
    ],
    "env": {
      "DEBUG": "False",
      "DJANGO_SETTINGS_MODULE": "socialdots.settings",
      "PYTHONPATH": "/var/task",
      "VERCEL_REBUILD_TRIGGER": "20250909-153000",
      "DEPLOYMENT_VERSION": "v2.0"
    },
    "functions": {
      "wsgi_handler.py": {
        "maxDuration": 30
      }
    },
    "regions": ["iad1"]
}