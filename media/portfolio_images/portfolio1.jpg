<!-- This is a placeholder SVG that looks like an Instagram story -->
<svg xmlns="http://www.w3.org/2000/svg" width="300" height="500" viewBox="0 0 300 500">
  <!-- Story background with gradient -->
  <defs>
    <linearGradient id="storyGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#27548A;stop-opacity:0.8" />
      <stop offset="100%" style="stop-color:#DDA853;stop-opacity:0.6" />
    </linearGradient>
  </defs>
  
  <rect width="300" height="500" fill="url(#storyGradient)"/>
  
  <!-- Story content -->
  <text x="150" y="250" font-family="Arial" font-size="24" fill="white" text-anchor="middle">Your Brand Story</text>
  
  <!-- Story elements -->
  <rect x="50" y="280" width="200" height="40" rx="20" fill="white" opacity="0.3"/>
  <text x="150" y="305" font-family="Arial" font-size="16" fill="white" text-anchor="middle">Swipe Up</text>
  
  <!-- Story decorative elements -->
  <circle cx="50" cy="50" r="20" fill="white" opacity="0.2"/>
  <circle cx="250" cy="450" r="30" fill="white" opacity="0.2"/>
  <circle cx="200" cy="100" r="15" fill="white" opacity="0.2"/>
  <circle cx="100" cy="400" r="25" fill="white" opacity="0.2"/>
</svg>