<!-- This is a placeholder SVG that looks like a social media post -->
<svg xmlns="http://www.w3.org/2000/svg" width="400" height="400" viewBox="0 0 400 400">
  <!-- Post background -->
  <rect width="400" height="400" fill="#f3f4f6"/>
  
  <!-- Post content -->
  <rect x="50" y="50" width="300" height="300" rx="5" fill="#DDA853" opacity="0.2"/>
  
  <!-- Post elements -->
  <rect x="80" y="100" width="240" height="10" rx="5" fill="#27548A" opacity="0.5"/>
  <rect x="80" y="120" width="180" height="10" rx="5" fill="#27548A" opacity="0.5"/>
  <rect x="80" y="140" width="210" height="10" rx="5" fill="#27548A" opacity="0.5"/>
  
  <!-- Post image placeholder -->
  <rect x="80" y="170" width="240" height="120" rx="5" fill="#27548A" opacity="0.3"/>
  
  <!-- Like, comment, share icons -->
  <circle cx="100" cy="320" r="10" fill="#27548A" opacity="0.6"/>
  <circle cx="140" cy="320" r="10" fill="#27548A" opacity="0.6"/>
  <circle cx="180" cy="320" r="10" fill="#27548A" opacity="0.6"/>
</svg>